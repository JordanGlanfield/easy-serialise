{
    "name": "easy-serialise",
    "version": "0.0.1",
    "description": "A library for serialising complex JS object structures into JSON and loading them into their original form. Resolves cyclic dependencies, retains type information, and serialises by reference rather than copy.",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "test": "vitest",
        "lint": "eslint .",
        "lint:fix": "eslint --fix",
        "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
    },
    "keywords": [
        "json",
        "js",
        "object",
        "class",
        "serialise",
        "deserialise",
        "parse",
        "unparse"
    ],
    "author": "Jordan",
    "license": "ISC",
    "devDependencies": {
        "@eslint/js": "^9.0.0",
        "eslint": "^9.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "globals": "^15.0.0",
        "prettier": "^3.2.5",
        "vitest": "^1.5.0"
    }
}
